programa {
   funcao inicio() {
       
       cadeia nome, sobrenome, ultimoNome

        escreva("Digite seu nome: ")
        leia(nome)
        escreva("Digite seu sobrenome: ")
        leia(sobrenome)
        escreva("Digite seu último nome: ")
        leia(ultimoNome)
        escreva("Seu nome completo é: ", nome, sobrenome, ultimoNome)
   }
}