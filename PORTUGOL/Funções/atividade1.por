programa {
   funcao inicio() {
    
        /*Crie um programa que possua uma função separador e a chame na função inicio antes e depois de imprimir “Olá Mundo!”. 
        A função separador deve imprimir “---”.*/

        separador()
        escreva("Olá Mundo!\n")
        separador()
   }

   funcao separador() {
        escreva("---\n")
   }
}